<VBox prefHeight="618.0" prefWidth="993.0" xmlns="http://javafx.com/javafx/21" xmlns:fx="http://javafx.com/fxml/1" fx:controller="Project_I_Code.GUI.MainController">
  <children>
    <MenuBar VBox.vgrow="NEVER">
      <menus>
        <Menu mnemonicParsing="false" text="Help">
          <items>
            <MenuItem mnemonicParsing="false" text="About MyHelloApp" />
          </items>
        </Menu>
      </menus>
    </MenuBar>
    <AnchorPane maxHeight="-1.0" maxWidth="-1.0" prefHeight="550.0" prefWidth="906.0" VBox.vgrow="ALWAYS">
         <children>
            <TabPane prefHeight="408.0" prefWidth="343.0" tabClosingPolicy="UNAVAILABLE">
              <tabs>
                <Tab text="Standard">
                  <content>
                    <AnchorPane fx:id="standardMIBTreePanel" minHeight="0.0" minWidth="0.0" prefHeight="375.0" prefWidth="343.0" />
                  </content>
                     <content>
                        <AnchorPane fx:id="standardMIBTreePanel" />
                     </content>
                </Tab>
                <Tab text="Vendors Specific">
                  <content>
                    <AnchorPane minHeight="0.0" minWidth="0.0" prefHeight="180.0" prefWidth="200.0">
                           <children>
                              <ComboBox fx:id="cbChooseVendors" layoutY="-6.0" onMouseClicked="#chooseVendorsClicked" prefHeight="32.0" prefWidth="343.0" />
                              <AnchorPane layoutY="64.0" prefHeight="314.0" prefWidth="349.0">
                                 <children>
                                    <FlowPane fx:id="vendorMIBs" layoutY="-38.0" prefHeight="352.0" prefWidth="343.0">
                                       <padding>
                                          <Insets top="5.0" />
                                       </padding>
                                    </FlowPane>
                                 </children>
                              </AnchorPane>
                           </children></AnchorPane>
                  </content>
                </Tab>
              </tabs>
            </TabPane>
            <GridPane focusTraversable="true" gridLinesVisible="true" layoutY="408.0" prefHeight="182.0" prefWidth="343.0">
              <columnConstraints>
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="166.0" minWidth="10.0" prefWidth="83.0" />
                <ColumnConstraints hgrow="SOMETIMES" maxWidth="271.0" minWidth="10.0" prefWidth="260.0" />
              </columnConstraints>
              <rowConstraints>
                <RowConstraints maxHeight="23.0" minHeight="10.0" prefHeight="22.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="36.0" minHeight="10.0" prefHeight="26.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="46.0" minHeight="10.0" prefHeight="26.0" vgrow="SOMETIMES" />
                <RowConstraints maxHeight="48.0" minHeight="10.0" prefHeight="30.0" vgrow="SOMETIMES" />
                  <RowConstraints maxHeight="80.0" minHeight="10.0" prefHeight="76.0" vgrow="SOMETIMES" />
              </rowConstraints>
               <children>
                  <Label alignment="CENTER" prefHeight="21.0" prefWidth="83.0" text="Name" />
                  <Label fx:id="lbName" prefHeight="26.0" prefWidth="261.0" GridPane.columnIndex="1">
                     <GridPane.margin>
                        <Insets left="5.0" />
                     </GridPane.margin>
                  </Label>
                  <Label alignment="CENTER" prefHeight="25.0" prefWidth="84.0" text="Access" GridPane.rowIndex="1" />
                  <Label fx:id="lbAccess" prefHeight="33.0" prefWidth="259.0" GridPane.columnIndex="1" GridPane.rowIndex="1">
                     <padding>
                        <Insets left="5.0" />
                     </padding>
                  </Label>
                  <Label alignment="CENTER" prefHeight="28.0" prefWidth="82.0" text="Status" GridPane.rowIndex="2" />
                  <Label alignment="CENTER" prefHeight="23.0" prefWidth="83.0" text="Type" GridPane.rowIndex="3" />
                  <Label alignment="CENTER" prefHeight="71.0" prefWidth="83.0" text="Description" GridPane.rowIndex="4" />
                  <Label fx:id="lbStatus" prefHeight="26.0" prefWidth="270.0" GridPane.columnIndex="1" GridPane.rowIndex="2">
                     <GridPane.margin>
                        <Insets left="5.0" />
                     </GridPane.margin>
                  </Label>
                  <Label fx:id="lbType" prefHeight="23.0" prefWidth="259.0" GridPane.columnIndex="1" GridPane.rowIndex="3">
                     <padding>
                        <Insets left="5.0" />
                     </padding>
                  </Label>
                  <TextArea fx:id="taDescription" prefHeight="200.0" prefWidth="200.0" wrapText="true" GridPane.columnIndex="1" GridPane.rowIndex="4" />
               </children>
            </GridPane>
            <AnchorPane focusTraversable="true" layoutX="343.0" prefHeight="182.0" prefWidth="605.0">
               <children>
                  <Label layoutX="14.0" layoutY="14.0" text="IP Address">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <Label layoutX="262.0" layoutY="10.0" prefHeight="27.0" prefWidth="151.0" text="Community String">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfTargetIPAddress" cache="true" layoutX="107.0" layoutY="11.0" prefHeight="24.0" prefWidth="139.0" />
                  <PasswordField fx:id="tfCommunityString" cache="true" layoutX="413.0" layoutY="12.0" />
                  <Label alignment="CENTER" layoutX="33.0" layoutY="57.0" prefHeight="40.0" prefWidth="48.0" text="OID">
                     <font>
                        <Font name="System Bold" size="14.0" />
                     </font>
                  </Label>
                  <TextField fx:id="tfOID" cache="true" layoutX="107.0" layoutY="65.0" prefHeight="24.0" prefWidth="476.0" />
                  <Button fx:id="btnSNMPGet" layoutX="27.0" layoutY="122.0" mnemonicParsing="false" onMouseClicked="#SNMPGetButtonClicked" prefHeight="33.0" prefWidth="59.0" text="Get">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button layoutX="107.0" layoutY="122.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="82.0" text="Get Next">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button fx:id="SNMPWalkbtn" layoutX="203.0" layoutY="122.0" mnemonicParsing="false" onMouseClicked="#SNMPWalkButtonCLicked" prefHeight="33.0" prefWidth="82.0" text="Get Walk">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
                  <Button layoutX="303.0" layoutY="122.0" mnemonicParsing="false" prefHeight="33.0" prefWidth="59.0" text="Set">
                     <font>
                        <Font size="14.0" />
                     </font>
                  </Button>
               </children>
            </AnchorPane>
            <AnchorPane layoutX="343.0" layoutY="182.0" prefHeight="408.0" prefWidth="605.0">
               <children>
                  <TableView fx:id="queryTable" editable="true" prefHeight="408.0" prefWidth="622.0">
                    <columns>
                      <TableColumn fx:id="nameColumn" prefWidth="134.0" text="Name" />
                      <TableColumn fx:id="valueColumn" prefWidth="364.0" text="Value" />
                        <TableColumn fx:id="dataTypeColumn" prefWidth="130.0" text="Data Type" />
                    </columns>
                  </TableView>
               </children>
            </AnchorPane>
            <FlowPane layoutX="964.0" layoutY="182.0" prefHeight="408.0" prefWidth="36.0">
               <children>
                  <Label alignment="CENTER" onMouseClicked="#clearTableClicked" prefHeight="32.0" prefWidth="36.0" text="X" textFill="#f20000">
                     <font>
                        <Font name="System Bold" size="21.0" />
                     </font>
                     <FlowPane.margin>
                        <Insets top="30.0" />
                     </FlowPane.margin>
                  </Label>
               </children>
            </FlowPane>
         </children>
    </AnchorPane>
  </children>
</VBox>
